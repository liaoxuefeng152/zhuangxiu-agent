.profile-page-outer{background:#f3f4f6;min-height:100vh}.profile-page{padding-bottom:32rpx}.profile-page .header-banner{align-items:center;background:linear-gradient(135deg,#007aff,#0051d5);display:flex;flex-direction:column;height:200rpx;padding:30rpx 24rpx 24rpx;position:relative}.profile-page .header-banner .my-equity{color:#fff;font-size:24rpx;position:absolute;right:24rpx;top:30rpx}.profile-page .header-banner .avatar-wrap{align-items:center;background:hsla(0,0%,100%,.3);border-radius:50%;display:flex;height:80rpx;justify-content:center;margin-bottom:12rpx;width:80rpx}.profile-page .header-banner .avatar-placeholder{font-size:40rpx}.profile-page .header-banner .nickname{color:#fff;font-size:28rpx;margin-bottom:8rpx}.profile-page .header-banner .member-badge{color:gold;font-size:24rpx}.profile-page .header-banner .login-cta{align-items:center;display:flex;flex-direction:column}.profile-page .header-banner .login-cta .login-text{color:hsla(0,0%,100%,.9);font-size:28rpx;margin-bottom:16rpx}.profile-page .header-banner .login-cta .login-btn{background:#fff;border-radius:32rpx;color:#1677ff;font-size:28rpx;font-weight:600;padding:24rpx 56rpx}.profile-page .section{background:#fff;border-radius:16rpx;box-shadow:0 1rpx 3rpx rgba(0,0,0,.04);margin:0 16rpx 12rpx;padding:4rpx 20rpx}.profile-page .section .folder-item{align-items:center;border-bottom:1rpx solid #e5e7eb;display:flex;min-height:88rpx;padding:28rpx 0}.profile-page .section .folder-item:last-child{border-bottom:none}.profile-page .section .folder-item .folder-icon{font-size:40rpx;margin-right:20rpx}.profile-page .section .folder-item .folder-name{color:#1f2937;flex:1;font-size:28rpx}.profile-page .section .folder-item .arrow{color:#9ca3af;font-size:32rpx}.profile-page .section .section-header{align-items:center;display:flex;justify-content:space-between;margin-bottom:16rpx}.profile-page .section .section-header .section-title{color:#1f2937;font-size:19rpx;font-weight:700}.profile-page .section .section-header .filter{color:#1677ff;font-size:16rpx}.profile-page .section .photo-grid{display:flex;flex-wrap:wrap;gap:12rpx}.profile-page .section .photo-grid .photo-item{align-items:center;background:#fafbfc;border-radius:12rpx;display:flex;flex-direction:column;padding:12rpx 4rpx;width:calc(20% - 10rpx)}.profile-page .section .photo-grid .photo-item .photo-icon{font-size:26rpx;margin-bottom:6rpx}.profile-page .section .photo-grid .photo-item .photo-name{color:#6b7280;font-size:14rpx}.profile-page .menu-section{background:#fff;border-radius:16rpx;box-shadow:0 1rpx 3rpx rgba(0,0,0,.04);margin:0 16rpx 12rpx;padding:4rpx 20rpx}.profile-page .menu-section .menu-item{align-items:center;border-bottom:1rpx solid #e5e7eb;display:flex;padding:16rpx 0}.profile-page .menu-section .menu-item:last-child{border-bottom:none}.profile-page .menu-section .menu-item:active{opacity:.7}.profile-page .menu-section .menu-item .menu-icon{font-size:20rpx;margin-right:14rpx}.profile-page .menu-section .menu-item .menu-title{color:#1f2937;flex:1;font-size:15rpx}.profile-page .menu-section .menu-item .arrow{color:#9ca3af;font-size:16rpx}.profile-page .logout-section{margin-top:24rpx;padding:0 16rpx}.profile-page .logout-section .logout-btn{align-items:center;background:#fef2f2;border-radius:16rpx;display:flex;height:48rpx;justify-content:center;width:100%}.profile-page .logout-section .logout-btn:active{background:#fee2e2}.profile-page .logout-section .logout-btn .logout-text{color:#ff4d4f;font-size:15rpx;font-weight:600}.profile-page .version-info{margin-top:28rpx;text-align:center}.profile-page .version-info .version-text{color:#9ca3af;font-size:12rpx}