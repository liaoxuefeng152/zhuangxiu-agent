<view class="p18-page">
  <view class="report-list" wx:if="{{reports.length > 0}}">
    <view class="report-item" wx:for="{{reports}}" wx:key="id" bindtap="onReportTap" data-item="{{item}}">
      <text class="report-name">{{item.name}}</text>
      <text class="report-time">{{item.time}}</text>
      <text class="report-action">{{item.unlocked ? '查看' : '预览'}}</text>
    </view>
  </view>
  <view class="empty-state" wx:else>
    <text class="empty-text">暂无报告数据</text>
    <button class="btn-primary" bindtap="onGoGen">去生成</button>
  </view>
</view>
