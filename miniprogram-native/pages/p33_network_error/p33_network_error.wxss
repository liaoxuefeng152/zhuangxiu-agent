.p33-page {
  min-height: 100vh;
  background: #F8F8F8;
  display: flex;
  flex-direction: column;
  align-items: center;
  justify-content: center;
  padding: 60rpx;
}

.error-icon {
  font-size: 160rpx;
  color: #999;
  margin-bottom: 40rpx;
}

.error-title {
  font-size: 36rpx;
  font-weight: bold;
  color: #333;
  margin-bottom: 20rpx;
}

.error-desc {
  font-size: 28rpx;
  color: #666;
  margin-bottom: 60rpx;
  text-align: center;
}

.btn-wrap {
  width: 90%;
  display: flex;
  flex-direction: column;
  gap: 24rpx;
}

.btn-wrap .btn-primary,
.btn-wrap .btn-secondary {
  width: 100%;
  height: 88rpx;
  line-height: 88rpx;
}
